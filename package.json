{
  "name": "plugin-based-framework",
  "version": "1.0.0",
  "description": "Plugin-based framework with chat plugin",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "nodemon --exec ts-node  src/server.ts",
    "dev:client": "cd client && yarn dev",
    "build": "yarn build:server && yarn build:client",
    "build:server": "tsc",
    "build:client": "cd client && yarn build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "init:db": "ts-node src/scripts/init-db.ts",
    "check:db": "ts-node src/scripts/check-db.ts",
    "start": "node dist/server.js",
    "lang": "ts-node tools/lang.ts"
  },
  "keywords": [
    "plugin",
    "framework",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "prisma": "5.22.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.0.1"
}
