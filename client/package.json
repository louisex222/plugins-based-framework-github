    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "hls.js": "^1.6.15",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.561.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.0",
    "react-resizable-panels": "^4.0.14",
    "react-router": "^7.10.1",
    "react-router-dom": "^7.10.1",
